{
  "name": "db-newspaper",
  "version": "1.0.0",
  "description": "Database for newspaper",
  "main": "index.js",
  "type": "module",
  "dependencies": {
    "pg": "^8.14.1",
    "sequelize": "^6.37.6"
  },
  "devDependencies": {
    "babel-register": "^6.26.0",
    "chai": "^4.3.10",
    "dotenv": "^16.4.5",
    "jest": "^29.7.0",
    "sequelize-cli": "^6.6.2",
    "sinon": "^17.0.1"
  },
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "migrate": "sequelize-cli db:migrate",
